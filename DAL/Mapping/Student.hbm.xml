<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="EntityLayer" namespace="EntityLayer">
	<class name="EntityLayer.Student" table="Student" dynamic-update="true" >
		<cache usage="read-write"/>
		<id name="StudentID" column="StudentID" type="int">
			<generator class="native" />
		</id>
		<property name="RollNo" column="RollNo" type="int" />
		<property name="Name" column="Name" type="string" />
	</class>
</hibernate-mapping>